{
  "name": "laravel-react-boilerplate",
  "license": "MIT",
  "author": "rohan.ka",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "http://tree.mn/rohan.ka/laravel-react-boilerplate.git"
  },
  "scripts": {
    "generate:app": "node scripts/generators/app/index.js",
    "dev": "build --app-src=resources/src --app-pattern='(^commons$)|(-app$)' --build-command='npm run dev --silent -- --hide-modules'",
    "load": "load --app-src=resources/src --app-pattern='(^commons$)|(-app$)'",
    "prod": "build --app-src=resources/src --app-pattern='(^commons$)|(-app$)' --build-command='npm run prod --silent'",
    "test": "build --app-src=resources/src --app-pattern='(^commons$)|(-app$)' --build-command='npm run test --silent'",
    "setup": "setup --app-src=resources/src --app-pattern='(^commons$)|(-app$)' --build-command='npm run dev'",
    "presetup": "yarn install || npm install",
    "generate": "plop --plopfile scripts/generators/index.js",
    "lint": "eslint --fix .",
    "clean:all": "rimraf public/build/*",
    "predev": "npm run clean:all",
    "preprod": "npm run clean:all",
    "composer": "composerjs",
    "lint:staged": "lint-staged",
    "remove:app": "node scripts/removeApp.js",
    "remove:example": "rimraf resources/src/example-app && rimraf app/Modules/Example"
  },
  "lint-staged": {
    "**/*.js": "lint"
  },
  "pre-commit": [
    "lint:staged"
  ],
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "6.26.0",
    "bootstrap": "4.0.0-alpha.6",
    "bootstrap-sass": "^3.3.7",
    "compression": "1.7.1",
    "express": "4.16.1",
    "fontfaceobserver": "2.0.13",
    "history": "4.7.2",
    "hoist-non-react-statics": "2.3.1",
    "immutable": "3.8.2",
    "invariant": "2.2.2",
    "ip": "1.1.5",
    "lodash": "4.17.4",
    "node-sass": "^4.5.3",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-helmet": "5.2.0",
    "react-loadable": "5.0.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "3.7.2",
    "redux-immutable": "4.0.0",
    "redux-saga": "0.15.6",
    "reselect": "3.0.1",
    "sanitize.css": "5.0.0",
    "warning": "3.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-dynamic-import-node": "1.1.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.9",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chalk": "^2.1.0",
    "circular-dependency-plugin": "4.2.1",
    "coveralls": "3.0.0",
    "cross-env": "^5.0.1",
    "css-loader": "0.28.7",
    "enzyme": "3.1.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-redux-saga": "^0.5.0",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "1.1.5",
    "hb-setup": "git+ssh://git@tree.mn:rohan.ka/hb-setup.git",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "2.30.1",
    "imports-loader": "0.7.1",
    "inquirer": "^3.2.3",
    "jest-cli": "21.2.1",
    "json-loader": "^0.5.7",
    "lint-staged": "^4.2.1",
    "minimist": "^1.2.0",
