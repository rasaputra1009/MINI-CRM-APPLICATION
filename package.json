    "ncp": "^2.0.0",
    "ngrok": "2.2.22",
    "node-plop": "0.9.0",
    "normalize-chunks-webpack-plugin": "https://github.com/rohan-ka/normalize-chunks-webpack-plugin.git",
    "null-loader": "0.1.1",
    "offline-plugin": "4.8.4",
    "plop": "^1.8.1",
    "postcss-loader": "^2.0.6",
    "pre-commit": "^1.2.2",
    "react-hot-loader": "next",
    "react-test-renderer": "16.0.0",
    "rimraf": "2.6.2",
